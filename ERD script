Departments
-
dept_no PK VARCHAR
dept_name VARCHAR

Department_Manager
- 
dept_no VARCHAR FK >- Department_Emp.dept_no
emp_no INT FK >- Employees.emp_no
from_date DATE
to_date DATE

Department_Emp
-
emp_no INT
dept_no VARCHAR FK >- Departments.dept_no
from_date DATE
to_date DATE

Employees
-
emp_no INT FK >- Department_Emp.emp_no
birth_date DATE
first_name VARCHAR
last_name VARCHAR
gender VARCHAR
hire_date DATE

Titles
-
emp_no INT FK >- Employees.emp_no
title VARCHAR
from_date DATE
to_date DATE

Salaries
-
emp_no INT FK >- Employees.emp_no
salary VARCHAR
from_date DATE
to_date DATE
